function result = classify(T, l, U, mew, F)
    result = 0;
    for i=1:10000
        x = T(:, i);
        d = 123456.1234;
        for j=1:10
           Uq_j = U(:, (j-1)*29+1:j*29);
           Fj = F(:, (j-1)*1600+1:j*1600);
           mew_j = mew(:, j);
           x_head = Uq_j * Fj + mew_j;
           ej = norm((x-x_head), 2);
           if (ej < d) 
               d = ej;
               class = j-1;
           end
        end
        if (l(i) == class)
            result = result + 1;
        else
            result = result -1;
        end
        printf("%d-th image - Actual: %d, Prediction: %d", i,class,l(i));
    end
    result = result / 10000 * 100;
end

